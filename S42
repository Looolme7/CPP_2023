#include <iostream> 
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
    int m[] = { 1, 8, 3, 6, 5, 4, 9, 2, 8, 12 };
    int size = 10;
    sort(m, m + size);
    reverse(m, m + size);

    for (int i = 0; i < size; i++) {
        cout << m[i] << " ";
    }
    return 0;
}
